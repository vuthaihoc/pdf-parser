{
  "name": "vuthaihoc/pdf-parser",
  "description": "PHP library to parse text from PDF files, use symfony/process 3.x",
  "keywords": ["pdf", "pdf-parser", "pdftotext", "parser"],
  "homepage": "https://github.com/wrseward/pdf-parser",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "wrseward",
      "email": "wrseward@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/process": "~2.8|~3.0|~3.1"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.2",
    "mikey179/vfsStream": "~1.5"
  },
  "autoload": {
    "psr-4": {
      "Wrseward\\PdfParser\\": "src/"
    }
  }
}
